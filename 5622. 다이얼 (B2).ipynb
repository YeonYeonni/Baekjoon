    "         'JKL', 'MNO', 'PQRS',\n",
    "         'TUV', 'WXYZ']\n",
    "\n",
    "s = input()\n",
    "\n",
    "time = 0\n",
    "\n",
    "for i in range(len(alpha)):\n",
    "    for j in range(len(s)):\n",
    "        if s[j] in alpha[i]:\n",
    "            time += (i+3)\n",
    "            \n",
    "print(time)"
