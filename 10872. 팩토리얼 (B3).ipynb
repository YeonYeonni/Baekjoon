    "n = int(input())\n",
    "\n",
    "def factorial(n):\n",
    "    \n",
    "    if n <= 1:\n",
    "        return 1    \n",
    "\n",
    "    return n * factorial(n - 1)\n",
    "\n",
    "print(factorial(n))"
   ]
