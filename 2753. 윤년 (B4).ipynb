   "source": [
    "year = int(input())\n",
    "\n",
    "def yoon(year):\n",
    "    if year % 4 == 0:\n",
    "        if year % 100 != 0:\n",
    "            return 1\n",
    "        if year % 400 == 0:\n",
    "            return 1\n",
    "        \n",
    "        else:\n",
    "            return 0\n",
    "    else:\n",
    "        return 0\n",
    "    \n",
    "print(yoon(year))"
   ]
  }
